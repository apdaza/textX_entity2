entity Persona {
  nombre : string       
  apellido : string     
  direccion: Direccion  
  edad: integer        
  telefono : Telefono  
  correo : string  
  apodo : string    
}                     

entity Direccion {
  calle : string
  ciudad : string
  pais : string
  numero : integer
}

entity Telefono {
  indicador : string
  numero : string
}

entity Compromiso {
  persona : Persona
  descripcion : string
}

entity Cargo {
  empresa : string
  jefe : Persona
  salario : integer
}
